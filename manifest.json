{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,

  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons":{
    "64": "icon.png"
  },
   "content_scripts": [
     {
     "matches": ["http://*/*","https://*/*"],
     "js": ["contentScript.js"]
     }
 ]
} 